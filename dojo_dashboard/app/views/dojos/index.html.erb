<h1>Listing <%= @count %> Dojos</h1>
<table>
    <th>Coding Dojo Branch</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
<% @dojos.each do |dojo| %>
<tr>
    <td><%= dojo.branch %></td>
    <td><%= dojo.street %></td>
    <td><%= dojo.city %></td>
    <td><%= dojo.state %></td>
    <td><button style="border-radius: 20px; background-color: blue; height: 25px;"><a href="/display/<%= dojo.id %>" style="color: white; text-decoration: none;">Show</a></button></td>
    <td><button style="border-radius: 20px; background-color: blue; height: 25px;"><a href="/edit/<%= dojo.id %>" style="color: white; text-decoration: none;">Edit</a></button></td>
    <td><button style="border-radius: 20px; background-color: blue; height: 25px;"><a href="/destroy/<%= dojo.id %>" style="color: white; text-decoration: none;">Delete</a></button></td>
</tr>
<% end %>
</table>

<button style="border-radius: 30px; background-color: blue; height: 50px;" ><a href="/dojos/new" style="color: white; text-decoration: none;">Create a new Dojo</a></button>