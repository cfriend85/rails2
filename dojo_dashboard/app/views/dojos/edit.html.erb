<h1><%= @dojo.branch %></h1>

<form action="/edit/<%= @dojo.id %>" method="POST">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    Coding Dojo Branch: <input type="text" name="branch" value="<%= @dojo.branch %>"><br>
    Street: <input type="text" name="street" value="<%= @dojo.street %>"><br>
    City: <input type="text" name="city" value="<%= @dojo.city %>"><br>
    State: <input type="text" name="state" value="<%= @dojo.state %>"><br>
    <input type="submit" value="Update Dojo" style="border-radius: 20px; background-color: blue; height: 25px; color: white">
</form>