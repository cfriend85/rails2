<h1>Branch Name: <%= @dojo.branch %></h1>
<h1>Address: <%= @dojo.street %></h1>
<h2>City: <%= @dojo.city%> State: <%= @dojo.state %></h2>
<button style="border-radius: 20px; background-color: blue; height: 25px; color: white"><a href="/" style="color: white; text-decoration: none;">Home</a></button><button style="border-radius: 20px; background-color: blue; height: 25px; color: white"><a href="/edit/<%= @dojo.id %>" style="color:white; text-decoration: none;"</style>Edit</a></button>

<h1>Students in: <%= @dojo.branch %></h1>

<table>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Actions</th>
<% @dojo.students.each do |student| %>
<tr>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><%= student.email %></td>
    <td><button style="border-radius: 20px; background-color: blue; height: 25px; color: white"><a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>" style="color: white; text-decoration: none;">Show</a></button></td>
    <td><button style="border-radius: 20px; background-color: blue; height: 25px; color: white"><a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>/edit" style="color: white; text-decoration: none;">Edit</a></button></td>
    <td><button style="border-radius: 20px; background-color: blue; height: 25px; color: white"><a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>" data-method="delete" style="color: white; text-decoration: none;">Delete</a></button></td>
</tr>
<% end %>
</table>
<button style="border-radius: 20px; background-color: blue; height: 25px; color: white"><a href="/" style="color: white; text-decoration: none;">Home</a></button><button style="border-radius: 20px; background-color: blue; height: 25px; color: white"><a href="/dojos/<%= @dojo.id %>/students/new" style="color:white; text-decoration: none;"</style>Add a Student</a></button>




